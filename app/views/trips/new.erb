<h1>Make a new trip</h1>
<form action="/trips/new" method="post">
  <p>Enter a new trip</p>

  <label for="duration">Duration</label>
  <input type='text' name='trip[duration]' placeholder="Duration"/><br/>

  <label for="start_date">Start Date</label>
  <input type='text' name='trip[start_date]' placeholder="Duration"/><br/>

  <label for="start_station_name">Start Station Name</label>
  <input type='text' name='trip[start_station_name]' placeholder=""/><br/>

  <label for="end_date">End Date</label>
  <input type='text' name='trip[end_date]' placeholder=""/><br/>

  <label for="end_station_name">End Station</label>
  <input type='text' name='trip[end_station_name]' placeholder=""/><br/>

  <label for="bike_id">Bike ID</label>
  <input type='text' name='trip[bike_id]' placeholder=""/><br/>

  <label for="subscription_type">Subscription</label>
  <input type='text' name='trip[subscription_type]' placeholder=""/><br/>

  <label for="zip_code">Zip Code</label>
  <input type='text' name='trip[zip_code]' placeholder=""/><br/>

  <input type='submit'/>
</form>
